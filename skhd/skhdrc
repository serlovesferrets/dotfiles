alt - j : yabai -m window --focus south
alt - k : yabai -m window --focus north
alt - h : yabai -m window --focus west
alt - l : yabai -m window --focus east

alt + shift - j : yabai -m window --swap south
alt + shift - k : yabai -m window --swap north
alt + shift - h : yabai -m window --swap west
alt + shift - l : yabai -m window --swap east

alt + ctrl - j : yabai -m window --warp south
alt + ctrl - k : yabai -m window --warp north
alt + ctrl - h : yabai -m window --warp west
alt + ctrl - l : yabai -m window --warp east

alt + shift - a : yabai -m window --resize left:-40:0
alt + shift - s : yabai -m window --resize bottom:0:40
alt + shift - w : yabai -m window --resize top:0:-40
alt + shift - d : yabai -m window --resize right:40:0

alt + shift - t : yabai -m window --toggle float --grid 4:4:1:1:2:2
alt + shift - m : yabai -m window --toggle zoom-fullscreen 

alt - 1 : yabai -m space --focus 1
alt - 2 : yabai -m space --focus 2
alt - 3 : yabai -m space --focus 3
alt - 4 : yabai -m space --focus 4
alt - 5 : yabai -m space --focus 5
alt - 6 : yabai -m space --focus 6
alt - 7 : yabai -m space --focus 7
alt - 8 : yabai -m space --focus 8
alt - 9 : yabai -m space --focus 9

alt + shift - 1 : yabai -m window --space 1
alt + shift - 2 : yabai -m window --space 2
alt + shift - 3 : yabai -m window --space 3
alt + shift - 4 : yabai -m window --space 4
alt + shift - 5 : yabai -m window --space 5
alt + shift - 6 : yabai -m window --space 6

alt + shift - n : yabai -m space --focus next || yabai -m space --focus first
alt + shift - p : yabai -m space --focus prev || yabai -m space --focus last

alt + shift - f : /Users/ser/.config/skhd/toggle_fullscreen.sh
alt + shift - r : yabai -m space --toggle padding; yabai -m space --toggle gap

